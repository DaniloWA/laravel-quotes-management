{
  "name": "danilowa/laravel-quotes-management",
  "description": "Laravel Quotes Master is a package for Laravel that allows you to fetch quotes from various quote APIs and store the retrieved data in the local database. It provides methods to retrieve quotes from the database based on different criteria, including author and length.",
  "type": "library",
  "keywords": [
    "LaravelQuotesManagement",
    "laravel",
    "Quotes"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Danilo Oliveira",
      "email": "daniloworkdev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Danilowa\\LaravelQuotesManagement\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^10.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "psr-4": {
    "App\\": "app/",
    "Danilowa\\LaravelQuotesManagement\\": "packages/laravel-quotes-management/src/"
  },
  "files": [
    "vendor/laravel/framework/src/Illuminate/Support/helpers.php"
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Danilowa\\LaravelQuotesManagement\\QuotesServiceProvider"
      ],
      "aliases": {
        "Quotes": "Danilowa\\LaravelQuotesManagement\\Quotes"
      }
    }
  }
}
